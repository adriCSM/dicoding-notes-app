{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "warn",
    "import/no-extraneous-dependencies": ["warn", { "devDependencies": true }],
    "object-curly-newline": "warn"
  }
}
