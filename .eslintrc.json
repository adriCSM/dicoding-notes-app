{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "warn",
    "import/no-extraneous-dependencies": ["warn", { "devDependencies": false }],
    "object-curly-newline": "off",
    "no-console": "warn",
    "import/order": "off",
    "max-len": ["error", { "code": 200 }]
  }
}
